# Example environment configuration for India MF Data PWA
# Copy to `.env` (never commit secrets) and fill with actual credentials.

# Server
PORT=3001
NODE_ENV=development
APP_VERSION=dev

# External APIs
MFAPI_BASE_URL=https://api.mfapi.in/mf
NEXT_PUBLIC_API_BASE_URL=https://india-mf-api.onrender.com
# Comma-separated; wildcard subdomains supported (e.g., https://*.vercel.app)
CORS_ALLOWED_ORIGINS=https://india-mf-data-pwa.vercel.app,https://*.vercel.app

# Frontend toggles
NEXT_TELEMETRY_DISABLED=1

# Datastores
DATABASE_URL=postgresql://user:password@hostname:5432/imfd_pg_db
REDIS_URL=
